days_of_week = ["Monday", "Tuesday", "Wednesday", "Thursday", "Friday", "Saturday"]

# Initialize an empty dictionary
schedule_dict = {[[x for x in range(1,8) for _ in daily_schedules]]}


# Use a for loop to create key-value pairs
for day_number in range(1, 31):
    # Use modulo operator to cycle through days of the week
    day_name = days_of_week[(day_number - 1) % len(days_of_week)]
    schedule_dict[day_number] = day_name

# Print the resulting dictionary
print(schedule_dict)
